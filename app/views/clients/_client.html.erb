<tr>
	<td>
	Client 
</td>
<td>
  <%= link_to client.name, client %>
</td>
<td>
	<%= client.email %>
</td>
<td>
	<%= client.phone %>
</td>
<td>
	<%= client.created_at %>
</td>
<td>
  <% if current_user.admin? && !current_user?(client) %>
  	<%= link_to "delete", client, method: :delete, 
  				data: { confirm: "You sure?" } %>
  <% end %>
 </td>
</tr>
